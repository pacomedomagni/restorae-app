# =============================================================================
# Restorae App Environment Configuration
# =============================================================================
# Copy this file to .env and fill in your values
# NEVER commit .env with real values to version control
# =============================================================================

# API Configuration
EXPO_PUBLIC_API_URL=http://localhost:3000/api/v1

# Sentry Error Tracking
# Get your DSN from https://sentry.io
EXPO_PUBLIC_SENTRY_DSN=YOUR_SENTRY_DSN

# RevenueCat In-App Purchases
# Get your keys from https://app.revenuecat.com
EXPO_PUBLIC_REVENUECAT_IOS=YOUR_REVENUECAT_IOS_KEY
EXPO_PUBLIC_REVENUECAT_ANDROID=YOUR_REVENUECAT_ANDROID_KEY

# Google OAuth
# Get your client IDs from https://console.cloud.google.com
EXPO_PUBLIC_GOOGLE_WEB_CLIENT_ID=YOUR_GOOGLE_WEB_CLIENT_ID
EXPO_PUBLIC_GOOGLE_IOS_CLIENT_ID=YOUR_GOOGLE_IOS_CLIENT_ID
EXPO_PUBLIC_GOOGLE_ANDROID_CLIENT_ID=YOUR_GOOGLE_ANDROID_CLIENT_ID
